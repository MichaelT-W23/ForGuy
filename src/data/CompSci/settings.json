    "Text": [
        {
            "page": "My Visual Studio Code Settings",
            "desc": "This is my settings file for VSCode. Contains mostly keyword coloring. Read the comments for more details.",
            "header": "Settings.json",
            "downloadBtn": "Download JSON",
            "copyBtn": "Copy JSON"
        }
    ],
    "settings": [
        {
            "text": "{"
        },
        {
            "text": "/**"
        },
        {
            "text": "How to Open this settings json file"
        },
        {
            "text": "1. Open Visual Studio code"
        },
        {
            "text": "2. Press command + shift + p"
        },
        {
            "text": "3. Lookup and click on the \"Preferences: Open Settings (JSON)\" option"
        },
        {
            "text": "in the drop down menu."
        },
        {
            "text": "*/"
        },
        {
            "text": "\"security.workspace.trust.untrustedFiles\": \"open\","
        },
        {
            "text": "//\"python.defaultInterpreterPath\": \"/usr/bin/python3\","
        },
        {
            "text": "//My Default python path for vs code"
        },
        {
            "text": "\"python.defaultInterpreterPath\": \"~/.pyenv/versions/3.10.6/bin/python\","
        },
        {
            "text": "//Work bench color theme"
        },
        {
            "text": "\"workbench.colorTheme\": \"Atom One Dark\","
        },
        {
            "text": "//Sets Tab Size to 4 spaces"
        },
        {
            "text": "\"editor.tabSize\": 4,"
        },
        {
            "text": "//Disables Run | Debug text"
        },
        {
            "text": "\"editor.codeLens\": false,"
        },
        {
            "text": "//Turns off weird coloring of Java brackets"
        },
        {
            "text": "\"editor.bracketPairColorization.enabled\": false,"
        },
        {
            "text": "//Sets the maximum number of items in a json file to 50,000 without"
        },
        {
            "text": "//warning about performance issues. Was originally 5,000."
        },
        {
            "text": "\"json.maxItemsComputed\": 50000,"
        },
        {
            "text": "//Disables yellow underline for reportMissingModuleSource"
        },
        {
            "text": "//reportMissingImports and errors in Python."
        },
        {
            "text": "// \"python.analysis.diagnosticSeverityOverrides\" : {"
        },
        {
            "text": "//     \"reportMissingModuleSource\" : \"none\","
        },
        {
            "text": "//     \"reportMissingImports\": \"none\""
        },
        {
            "text": "// },"
        },
        {
            "text": "//Opens Live Server in Chrome by Default"
        },
        {
            "text": "\"liveServer.settings.CustomBrowser\": \"chrome\","
        },
        {
            "text": "//Turns off the warning about unusual characters in a txt file."
        },
        {
            "text": "\"editor.unusualLineTerminators\" : \"off\","
        },
        {
            "text": "/** Gets rid of autofill parameter suggestions in method calls in java */"
        },
        {
            "text": "\"editor.inlayHints.enabled\": \"off\","
        },
        {
            "text": "//Changes font size to 13.5"
        },
        {
            "text": "\"editor.fontSize\": 13.5,"
        },
        {
            "text": "\"launch\": {"
        },
        {
            "text": "\"configurations\": [],"
        },
        {
            "text": "\"compounds\": []"
        },
        {
            "text": "},"
        },
        {
            "text": "// Set to false to show .git folder"
        },
        {
            "text": "\"files.exclude\": {"
        },
        {
            "text": "\"**/.git\": false,"
        },
        {
            "text": "},"
        },
        {
            "text": "/**"
        },
        {
            "text": "How to change a color"
        },
        {
            "text": "1. hover over the keyword whose color you want changed"
        },
        {
            "text": "2. Press command + shift + p on mac"
        },
        {
            "text": "3. Seach and click \"Developer: Inspect Editor Tokens and Scopes\""
        },
        {
            "text": "4. In the box that opens next to \"textmate scopes\" copy the first line"
        },
        {
            "text": "and paste it under the scope keyword that's above the color you want"
        },
        {
            "text": "to change the keyword too. If it doesn't work try the other lines."
        },
        {
            "text": "5. To stop inspecting the tokens press command + shift + p"
        },
        {
            "text": "then press enter"
        },
        {
            "text": "*/"
        },
